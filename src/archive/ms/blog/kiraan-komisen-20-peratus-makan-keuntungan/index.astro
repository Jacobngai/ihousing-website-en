---
import BlogPost from '@/layouts/BlogPost.astro';

const title = "Kiraan Komisen: Bagaimana 20% Makan Keuntungan Anda";
const description = "Perincian bagaimana yuran komisen 20% memakan keuntungan Airbnb dari masa ke masa. Lihat kos sebenar pengurusan berasaskan peratusan.";
const image = "/images/blog/kiraan-komisen-20-peratus.jpg";
const date = "2026-01-25";
const category = "pricing-comparison";
const language = "ms";

const content = "<h1>Kiraan Komisen: Bagaimana 20% Makan Keuntungan Anda</h1><p>Fahami kos sebenar yuran komisen 20%. Lihat bagaimana yuran peratusan memberi kesan dari masa ke masa dan kenapa harga yuran rata menjimatkan wang anda.</p>";
---
<BlogPost title={title} description={description} image={image} date={date} category={category} language={language} content={content} />
